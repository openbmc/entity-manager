    {
        "Exposes": [
            {
                "Address": "$address",
                "Bus": "$bus",
                "Name": "NVMe $index FRU",
                "Type": "EEPROM"
            },
            {
                "Address": "0x6a",
                "Bus": "$bus",
                "Name": "HDDBOARD0 SSD$bus - 48 / 2 TEMP_C",
                "PEC": "Required",
                "PowerState": "On",
                "Thresholds": [
                    {
                        "Direction": "greater than",
                        "Index": 2,
                        "Name": "upper non recoverable",
                        "Severity": 4,
                        "Value": 85
                    },
                    {
                        "Direction": "greater than",
