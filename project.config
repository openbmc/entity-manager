[access]
	inheritFrom = All-Projects
[access "refs/heads/*"]
	create = group github/openbmc/entity-manager-maintainers
	push = +force group github/openbmc/entity-manager-maintainers
	forgeCommitter = group github/openbmc/entity-manager-maintainers
	label-Code-Review = -2..+2 group github/openbmc/entity-manager-maintainers
	label-Ok-To-Test = +0..+1 group github/openbmc/entity-manager-maintainers
	label-Verified = -1..+1 group github/openbmc/entity-manager-maintainers
	submit = group github/openbmc/entity-manager-maintainers
	editTopicName = group github/openbmc/entity-manager-maintainers
[access "refs/tags/*"]
	push = +force group github/openbmc/entity-manager-maintainers
	pushTag = group github/openbmc/entity-manager-maintainers
	pushSignedTag = group github/openbmc/entity-manager-maintainers
