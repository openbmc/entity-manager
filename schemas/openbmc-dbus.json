{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Schemas for JSON representations of OpenBMC DBus interfaces",
    "definitions": {
        "xyz": {
            "openbmc_project": {
                "Inventory": {
                    "Decorator": {
                        "Asset": {
                            "additionalProperties": false,
                            "properties": {
                                "Manufacturer": {
                                    "type": "string"
                                },
                                "Model": {
                                    "type": "string"
                                },
                                "PartNumber": {
                                    "type": "string"
                                },
                                "SerialNumber": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "Manufacturer",
                                "Model",
                                "PartNumber",
                                "SerialNumber"
                            ],
                            "type": "object"
                        },
                        "AssetTag": {
                            "additionalProperties": false,
                            "properties": {
                                "AssetTag": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "AssetTag"
                            ],
                            "type": "object"
                        },
                        "I2CDevice": {
                            "additionalProperties": false,
                            "properties": {
                                "Address": {
                                    "$ref": "legacy.json#/definitions/Types/Address"
                                },
                                "Bus": {
                                    "$ref": "legacy.json#/definitions/Types/Bus"
                                },
                                "Name": {
                                    "$ref": "legacy.json#/definitions/Types/Name"
                                }
                            },
                            "required": [
                                "Address",
                                "Bus"
                            ],
                            "type": "object"

                        },
                        "Ipmi": {
                            "additionalProperties": false,
                            "properties": {
                                "EntityId": {
                                    "$ref": "legacy.json#/definitions/Types/EntityId"
                                },
                                "EntityInstance": {
                                    "$ref": "legacy.json#/definitions/Types/EntityInstance"
                                }
                            },
                            "required": [
                                "EntityId",
                                "EntityInstance"
                            ],
                            "type": "object"
                        }
                    },
                    "Item": {
                        "Board": {
                            "Motherboard": {
                                "additionalProperties": false,
                                "properties": {
                                    "ProductId": {
                                        "type": "number"
                                    }
                                },
                                "required": [
                                    "ProductId"
                                ],
                                "type": "object"
                            }
                        },
                        "Chassis": {
                            "additionalProperties": false,
                            "type": "object"
                        },
                        "Panel": {
                            "additionalProperties": false,
                            "type": "object"
                        },
                        "System": {
                            "additionalProperties": false,
                            "type": "object"
                        }
                    }
                }
            }
        }
    }
}
